{
  "version": 2,
  "builds": [
    {
      "src": "./main.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.12",
        "installCommand": "pip install -r requirements.txt --no-cache-dir"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/main.py"
    }
  ]
}